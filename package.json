{
  "name": "dictionary-static",
  "version": "1.0.0",
  "type": "module",
  "description": "Static dictionary website with semantic search",
  "scripts": {
    "dev": "vite",
    "dev:api": "wrangler pages dev dist --port 8788",
    "build": "npm run build:data && vite build",
    "build:data": "node build-scripts/parse-lift.js && node build-scripts/generate-embeddings.js",
    "build:embeddings": "node build-scripts/generate-embeddings.js",
    "test:embeddings": "node build-scripts/test-embeddings.js",
    "test:api": "node scripts/test-api.js",
    "preview": "vite preview",
    "deploy": "npm run build && wrangler pages deploy dist --project-name=dictionary-static"
  },
  "dependencies": {
    "@xenova/transformers": "^2.17.2",
    "lucide-vue-next": "^0.562.0",
    "vue": "^3.4.0",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.0",
    "@vueuse/head": "^2.0.0",
    "fast-xml-parser": "^4.3.4",
    "sharp": "^0.34.5",
    "vite": "^5.0.0",
    "vite-plugin-pwa": "^1.2.0",
    "vite-plugin-ssg": "^0.1.0"
  }
}
